script(type="text/javascript").
    function iqr(k) {
      return function(d, i) {
          var q1 = d.quartiles[0],
                  q3 = d.quartiles[2],
                          iqr = (q3 - q1) * k,
                                  i = -1,
                                          j = d.length;
                                              while (d[++i] < q1 - iqr);
                                                  while (d[--j] > q3 + iqr);
                                                      return [i, j];
                                                          };};

